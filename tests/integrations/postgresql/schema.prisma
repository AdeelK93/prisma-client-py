datasource db {
  provider = "postgres"
  url      = env("PRISMA_PY_POSTGRES_URL")
}

generator db {
  provider             = "python3 -m prisma"
  recursive_type_depth = 5
  http                 = "aiohttp"
}

model User {
  id         String   @id @default(cuid())
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  name       String
  role       Role     @default(USER)
}

enum Role {
  USER
  ADMIN
}
