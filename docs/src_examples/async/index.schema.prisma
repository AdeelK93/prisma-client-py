datasource db {
    // could be postgresql or mysql
    provider = "sqlite"
    url      = "file:dev.db"
}

generator db {
    provider = "prisma-client-py"
    http     = "aiohttp"
}

model Post {
    id         String   @default(cuid()) @id
    created_at DateTime @default(now())
    updated_at DateTime @updatedAt
    title      String
    published  Boolean
    desc       String?
}
